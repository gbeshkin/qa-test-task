<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <localRepository>/opt/maven/repository</localRepository>
    <proxies>
        <proxy>
            <active>true</active>
            <host>zscaler.proxy.int.kn</host>
            <port>80</port>
            <nonProxyHosts>*.int.kn</nonProxyHosts>
        </proxy>
    </proxies>
    <profiles>
        <profile>
            <id>public</id>
            <repositories>
                <repository>
                    <id>jasper</id>
                    <url>https://jaspersoft.artifactoryonline.com/jaspersoft/third-party-ce-artifacts</url>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>esb-releases</id>
                    <url>https://repository.int.kn/repository/esb</url>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>sso-releases</id>
                    <url>https://repository.int.kn/repository/sso</url>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>thirdparty</id>
                    <url>https://repository.int.kn/repository/thirdparty</url>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>cip</id>
                    <url>https://repository.int.kn/repository/cip</url>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>apache-releases</id>
                    <url>https://repository.int.kn/repository/apache-releases</url>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>cip-snapshots</id>
                    <url>https://repository.int.kn/repository/cip-snapshots</url>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>common</id>
                    <url>https://repository.int.kn/repository/maven/</url>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
            </repositories>
        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>public</activeProfile>
    </activeProfiles>
</settings>